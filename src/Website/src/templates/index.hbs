<!DOCTYPE html>
<html dir="ltr" lang="en-US">
	<head>
		<title>MindMuscle</title>

		<!-- Import standard header -->
		{{> header}}

		<link href="{{ assetPathCSS "sass/index.css" }}" rel="stylesheet" type="text/css">
	</head>
	<body>
		<header>
			<div class="mainIcon">
				<img src="{{ assetPathIMG "MindMuscle-logo.png" }}" alt="MindMuscle Logo">
			</div>
		</header>

		<div class="wrapper">
			<section class="diagram">
				<svg class="circle level1">
					<g>
						<circle r="150px" cx="0" cy="0" stroke-width="0" fill="#FC6F50" fill-opacity="0.6" />
						<text text-anchor="middle" x="0" y="-30" font-family="Avenir New" font-size="55" fill="#FFFFFF" fill-opacity="0.8"></text>
					</g>
				</svg>
				<svg class="circle level2">
					<g>
						<circle r="150px" cx="0" cy="0" stroke-width="0" fill="#77C7B0" fill-opacity="0.6" />
						<text text-anchor="middle" x="-60" y="40" font-family="Avenir New" font-size="55" fill="#FFFFFF" fill-opacity="0.8"></text>
					</g>
				</svg>
				<svg class="circle level3">
					<g>
						<circle r="150px" cx="0" cy="0" stroke-width="0" fill="#6AB1C5" fill-opacity="0.6" />
						<text text-anchor="middle" x="60" y="40" font-family="Avenir New" font-size="55" fill="#FFFFFF" fill-opacity="0.8"></text>
					</g>
				</svg>
				<div class="overall"></div>
			</section>

			<section class="table level1">
				<h1>Level 1</h1>
				<table>
					<tr>
						<th>Highest</th>
						<th>Average</th>
						<th>Score</th>
					</tr>
					<tr>
						<td class="highest">0</td>
						<td class="average">0</td>
						<td class="score">0</td>
					</tr>
				</table>
			</section>
			<section class="table level2">
				<h1>Level 2</h1>
				<table>
					<tr>
						<th>Highest</th>
						<th>Average</th>
						<th>Score</th>
					</tr>
					<tr>
						<td class="highest">0</td>
						<td class="average">0</td>
						<td class="score">0</td>
					</tr>
				</table>
			</section>
			<section class="table level3">
				<h1>Level 3</h1>
				<table>
					<tr>
						<th>Highest</th>
						<th>Average</th>
						<th>Score</th>
					</tr>
					<tr>
						<td class="highest">0</td>
						<td class="average">0</td>
						<td class="score">0</td>
					</tr>
				</table>
			</section>

			<section class="total"></section>
		</div>

		<script src="{{ assetPathJS "index.js" }}" type="text/javascript"></script>

		<!-- Import standard footer -->
		{{> footer}}
	</body>
</html>